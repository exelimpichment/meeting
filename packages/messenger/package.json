{
  "name": "@exelimpichment/messenger",
  "version": "0.0.5",
  "description": "Shared Prisma client and types for messenger service",
  "main": "dist/src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "dist/**/*",
    "generated/**/*",
    "src/index.d.ts",
    "src/types.ts"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "generate": "npx prisma generate --schema=../../apps/messenger/prisma/schema.prisma",
    "introspect": "npx prisma db pull",
    "prepublishOnly": "npm run clean && npm run generate && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exelimpichment/meeting.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@exelimpichment/meeting-api-gateway": "^0.0.2",
    "@exelimpichment/messenger": "^0.0.2",
    "@prisma/client": "6.6.0"
  },
  "devDependencies": {
    "prisma": "6.6.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "prisma",
    "messenger",
    "database",
    "orm"
  ],
  "author": "exelimpichment",
  "license": "MIT"
}
