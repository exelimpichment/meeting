generator client {
  provider = "prisma-client-js"
  output   = "../generated/iam-client"
}

generator packageClient {
  provider = "prisma-client-js"
  output   = "../../../../../packages/meeting-api-gateway/generated"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
