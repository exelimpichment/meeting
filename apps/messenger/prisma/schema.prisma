generator client {
  provider = "prisma-client-js"
  output   = "../generated/messenger-client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model messages {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  message   String
}
